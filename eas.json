{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "credentialsSource": "remote",
      "env": {
        "APP_DEBUG": "true"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      }
    },
    "preview": {
      "distribution": "internal",
      "credentialsSource": "remote",
      "env": {
        "APP_DEBUG": "true"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "distribution": "store",
      "credentialsSource": "remote",
      "env": {
        "APP_DEBUG": "false"
      },
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": "buildNumber"
      }
    },
    "production-apk": {
      "distribution": "internal",
      "credentialsSource": "remote",
      "env": {
        "APP_DEBUG": "false"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      }
    }
  },
  "submit": {
    "production": {}
  },
  "cli": {
    "appVersionSource": "remote",
    "version": "16.32.0"
  }
}
